    "import time as tm\n",
    "a=int(input('time :'))\n",
    "def countdown(t):\n",
    "    while t!=0:\n",
    "        m,s = divmod(t,60)\n",
    "        print(\"{:02d}:{:02d}\".format(m,s),end='\\r')\n",
    "        tm.sleep(1)\n",
    "        t-=1\n",
    "    print('fire in the hole')\n",
    "    \n",
    "countdown(a)"
   ]
